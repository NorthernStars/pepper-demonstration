    def onInput_onStart(self):
        file = self.getParameter("URL")
        url = self.getParameter("URL")
        if self.getParameter("URL is Local File"):
            url = self.behaviorAbsolutePath().split("apps/")
            if len(url) > 1:
                url = str( url[1].split("/")[0] ) + "/"

            lang = self.tts.getLanguage()
